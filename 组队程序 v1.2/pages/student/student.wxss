/* pages/student/student.wxss */
page {
  height: 100vh;
  display: flex;
  flex-direction: column;
  background-color: #eeeaea;
}

.container {
  padding: 20px;
  display: flex;
  flex-direction: column;
  gap: 10px;
}

.clickable-box {
  width: 100%;
  height: 100px;
  background-color: #ffffff;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #000;
  text-align: center;
  border-radius: 10px;
  cursor: pointer;
}